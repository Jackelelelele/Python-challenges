def insertionSort(mylist):
  for i in range(1, len(mylist)):
    key = mylist[i]
    j = i-1
    while j >= 0 and key < mylist[j] :
      mylist[j + 1] = mylist[j]
      j -= 1
    mylist[j + 1] = key
mylist = []
searchnumber = 0
min = 0
max = len(mylist)
found = False
guess = int((min+max)/2)
count = 0
while found == False and min <= max and count < 5:
  guess = int((min+max)/2)
  if mylist[guess] == searchnumber:
    found = True
    print(searchnumber)
  elif mylist[guess] > searchnumber:
    max = guess
  else:
    min = guess
  count = count + 1
if found == False:
  mylist = insertionSort(mylist)
min = 0
max = len(mylist)
count = 0
while found == False and min <= max and count < 5:
  guess = int((min+max)/2)
  if mylist[guess] == searchnumber:
    found = True
    print(searchnumber)
  elif mylist[guess] > searchnumber:
    max = guess
  else:
    min = guess
  count = count + 1
if found == False:
  print('Not found')
